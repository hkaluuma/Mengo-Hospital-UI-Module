<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.openmrs.module.covidsymptom">
    <class name="covidsymptom"
        table="covidsymptom_Symptom">
        <id name="symptomId" type="int" column="symptom_id" unsaved-value="0">
            <generator class="native" />
        </id>
        <discriminator column="symptom_id_id" insert="false" />
        <property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" />
        <property name="name" type="java.lang.String" column="name" length="255" unique="true" />
        <property name="description" type="java.lang.String" column="description" length="255" />    
    </class>
</hibernate-mapping>